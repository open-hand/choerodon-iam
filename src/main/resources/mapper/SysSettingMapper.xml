<mapper namespace="io.choerodon.iam.infra.mapper.SysSettingMapper">
    <select id="queryByKey" resultType="io.choerodon.iam.infra.dto.SysSettingDTO">
        SELECT *
        FROM iam_sys_setting
        WHERE SETTING_KEY = #{key}
    </select>
</mapper>